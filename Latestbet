      var idleTime = 0;



      function loadData() {

        // Increment the idle time counter every minute.

        var idleInterval = setInterval(timerIncrement, 60000); // 1 minute

      }



      function mouseMove() {

        idleTime = 0;

      }



      function keyPress() {

          idleTime = 0;

      }



      function timerIncrement() {

        idleTime = idleTime + 1;



        if (idleTime > 30) { // 30 minutes

            window.location.reload();

            idleTime = 0;

        }

      }

    </script>



